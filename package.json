{
  "name": "oncohelp-core",
  "version": "0.0.0",
  "description": "Backend for Oncohelp",
  "author": "Igor Kamyshev <igor@kamyshev.me>",
  "scripts": {
    "start:dev": "NODE_ENV=development nodemon",
    "start:debug": "NODE_ENV=development nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && tsc",
    "start:prod": "pm2 start ecosystem.config.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest --config ./jest.json",
    "test:watch": "jest --config ./jest.json --watch",
    "test:cov": "jest --config ./jest.json --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "types": "tsc --noEmit",
    "pretty": "prettier --write \"{,!(dist|coverage)/**/}*.{ts,tsx,js}\"",
    "cmd:dev": "ts-node -r tsconfig-paths/register ./src/console.ts",
    "cmd": "node ./tsconfig-paths-bootstrap.js ./dist/console.js",
    "evolutions:init": "evolutions --init",
    "evolutions:run": "evolutions"
  },
  "dependencies": {
    "@breadhead/detil-ts": "^1.0.1",
    "@breadhead/nest-throwable-bus": "^1.0.5",
    "@breadhead/red-sms-client": "^1.0.1",
    "@breadhead/s3-uploader": "^1.2.7",
    "@nestjs/common": "^5.1.0",
    "@nestjs/core": "^5.1.0",
    "@nestjs/cqrs": "^5.1.1",
    "@nestjs/jwt": "^0.2.0",
    "@nestjs/passport": "^5.1.0",
    "@nestjs/swagger": "^2.5.1",
    "@nestjs/typeorm": "^5.2.2",
    "@solid-soda/config": "^1.1.0",
    "@solid-soda/console": "^1.0.0",
    "@solid-soda/evolutions": "^0.0.10",
    "@solid-soda/voters": "^1.0.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "chalk": "^2.4.1",
    "class-transformer": "^0.1.10",
    "class-validator": "^0.9.1",
    "cors": "^2.8.4",
    "cron": "^1.6.0",
    "date-fns": "^1.30.1",
    "dotenv": "^6.2.0",
    "express-rate-limit": "^3.5.1",
    "inline-css": "^2.4.1",
    "json2csv": "^4.3.5",
    "lodash": "^4.17.11",
    "md5": "^2.2.1",
    "moment": "^2.22.2",
    "mysql": "^2.16.0",
    "nanoevents": "^1.0.5",
    "nanoid": "^1.2.6",
    "nodemailer": "^4.6.8",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "pm2": "^3.1.3",
    "redis": "^2.8.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.2.2",
    "trello": "^0.9.1",
    "tsconfig-paths": "^3.6.0",
    "tsoption": "^0.7.0",
    "twig": "^1.12.0",
    "typeorm": "^0.2.7",
    "typescript": "^3.0.1",
    "winston": "^3.1.0",
    "z": "^1.0.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.3.1",
    "@nestjs/testing": "^5.1.0",
    "@solid-soda/tslint-config": "^2.5.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/cron": "^1.6.0",
    "@types/express": "^4.16.0",
    "@types/express-rate-limit": "^3.3.0",
    "@types/jest": "^23.3.1",
    "@types/json2csv": "^4.2.0",
    "@types/lodash": "^4.14.116",
    "@types/md5": "^2.1.33",
    "@types/node": "^10.7.1",
    "@types/nodemailer": "^4.6.5",
    "@types/passport-http-bearer": "^1.0.33",
    "@types/passport-jwt": "^3.0.1",
    "@types/redis": "^2.8.10",
    "@types/supertest": "^2.0.5",
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^1.2.0",
    "jest": "^23.5.0",
    "lint-staged": "^8.1.0",
    "nodemon": "^1.18.3",
    "prettier": "^1.15.3",
    "rimraf": "^2.6.2",
    "supertest": "^3.1.0",
    "ts-jest": "^23.1.3",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0"
  },
  "resolutions": {
    "**/event-stream": "^4.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn test && yarn types",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "yarn pretty",
      "yarn lint",
      "git add"
    ]
  },
  "prettier": {
    "tabWidth": 2,
    "semi": false,
    "trailingComma": "all",
    "singleQuote": true
  }
}
